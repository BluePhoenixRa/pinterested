<p id="notice"><%= notice %></p>


<%= image_tag @pin.image.url %>
  
<p>
  <strong>Description:</strong>
  <%= @pin.description %>
</p>

<p>
  <strong>Pinned by:</strong>
  <%= @pin.user.name %>
</p>

<% if @pin.user == current_user %>
  <%= link_to 'Edit', edit_pin_path(@pin) %> |
<% end %>
<%= link_to 'Back', pins_path %>
